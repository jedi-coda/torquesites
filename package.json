{
    "name": "torquesites",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 3001",
        "predev": "npx kill-port 3001 || exit 0",
        "dev:3000": "next dev -p 3000",
        "predev:3000": "npx kill-port 3000 || exit 0",
        "dev:port": "node -e \"const p=process.env.PORT||3001; console.log('Starting on port',p);\" && next dev -p %PORT%",
        "predev:port": "node -e \"const p=process.env.PORT||3001; console.log('Killing port',p);\" && npx kill-port %PORT% || exit 0",
        "dev:clean": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true});\" && next dev -p 3000",
        "build": "next build"
    },
    "dependencies": {
        "@stripe/stripe-js": "^7.9.0",
        "@supabase/supabase-js": "^2.58.0",
        "autoprefixer": "^10.4.21",
        "clsx": "^2.1.1",
        "lucide-react": "^0.544.0",
        "next": "15.5.4",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "resend": "^6.1.1",
        "tailwindcss": "^3.4.18"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "cheerio": "^1.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^9",
        "eslint-config-next": "15.5.4",
        "rimraf": "^5.0.5",
        "ts-node": "^10.9.2",
        "typescript": "^5",
        "undici": "^6.19.8"
    }
}
